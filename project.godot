; Engine configuration file.
; It's best edited using the editor UI and not directly.
; Only a few essential settings are defined here to make
; this a runnable project. Further tuning can be done in
; the Godot editor once the project is opened.

config_version=4

;; Application settings
[application]
config/name="Stick Castle Defense"
config/description="Castle defense game with stick figure races."
run/main_scene="res://scenes/Main.tscn"

;; Display settings
[display]
window/size/viewport_width=960
window/size/viewport_height=540
window/stretch/mode="2d"
window/stretch/aspect="keep"

;; Rendering quality settings
[rendering]
quality/2d/antialiasing=2
